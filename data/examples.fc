fun fac(n) {
  if (n==0) r = 1;
  else r = n*fac(n-1);
  return r;
}

fun fib(n) {
  if (n==0) r = 0;
  else if (n==1) r = 1;
  else r = fib(n-1) + fib(n-2);
  return r;
}

fun fibFast(n) {
  fn = 1;
  fn2 = 0;
  while (n > 1) {
    fn3 = fn;
    fn = fn+fn2;
    fn2 = fn3;
    n = n-1;
  }
  if (n == 0) r = 0;
  else r = fn;
  return fn;
}

print fac(5);
print fib(10);
print fibFast(10);
